<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jk.mapper.AnMapper">


    <insert id="addgongsi" parameterType="CompanyModel">
         insert into t_company (companyName,gsjian,gsphone,gspople,gsweb,gszong,gsjieshao,address,serve,gsadrivs,goodsImg) values
         (#{companyName},#{gsjian},#{gsphone},#{gspople},#{gsweb},#{gszong},#{gsjieshao},#{address},#{serve},#{gsadrivs},#{goodsImg})
     </insert>

</mapper>